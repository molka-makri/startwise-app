      <string notr="true">/* Fond blanc pour le contenu des onglets avec bordure renforcée */
QTabWidget::pane {
    background-color: rgb(255, 255, 255); /* Fond blanc */
    border: 2px solid #888;
    border-radius: 10px;
    padding: 10px;
    margin-top: 5px; /* Espacement entre les onglets et le contenu */
}

/* Style des onglets */
QTabBar::tab {
    background: rgb(230, 230, 230); /* Couleur de fond des onglets (gris clair) */
    border: 2px solid #666;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    padding: 8px 16px;
    font-size: 14px;
    font-weight: bold; /* Titres en gras */
    margin-right: 4px; /* Espacement entre les onglets */
    transition: background 0.3s ease; /* Transition fluide */
}

/* Style des onglets sélectionnés */
QTabBar::tab:selected {
    background: #0078d4;  /* Couleur de fond quand l'onglet est sélectionné */
    color: white;
    font-weight: bold;
    border-bottom: 3px solid #0078d4; /* Ligne en bas pour l'onglet actif */
}

/* Style des onglets au survol */
QTabBar::tab:hover {
    background: #005a9e; /* Couleur de fond quand l'onglet est survolé */
    color: white;
}

/* Style des onglets non sélectionnés */
QTabBar::tab:!selected {
    margin-top: 2px;
}

/* Style général du widget */
QWidget {
    background-color: rgb(255, 255, 255);
    color: #333333;
    font-size: 16px;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
}

/* Style pour les tableaux */
QTableWidget {
    border: 2px solid #888;
    gridline-color: #666;
}

QHeaderView::section {
    background-color: #0078d4;
    color: white;
    font-weight: bold;
    border: 1px solid #666;
    padding: 8px;
}

/* Style pour les boutons plus visibles */
QPushButton {
    background-color: #0078d4;
    color: white;
    padding: 12px 24px;
    border-radius: 8px;
    border: 2px solid #005a9e;
    font-size: 16px;
    font-weight: bold;
    cursor: pointer;
    transition: background 0.3s ease, box-shadow 0.3s ease;
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
}

QPushButton:hover {
    background-color: #005a9e;
    box-shadow: 0px 6px 8px rgba(0, 0, 0, 0.25);
}

QPushButton:pressed {
    background-color: #003d6a;
    box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.2);
}
QPushButton#btnSupprimer {
    background-color: #d32f2f;
    border: 2px solid #b71c1c;
}

QPushButton#btnSupprimer:hover {
    background-color: #b71c1c;
}

/* Style pour les icônes des boutons Modifier et Supprimer */
QPushButton#btnModifier {
    background-color: transparent;
    border: none;
}

QPushButton#btnModifier:hover {
    background-color: rgba(0, 0, 0, 0.1);
}

QPushButton#btnSupprimer {
    background-color: transparent;
    border: none;
}

QPushButton#btnSupprimer:hover {
    background-color: rgba(255, 0, 0, 0.1);
}

/* Style pour les champs de saisie avec bordure */
QLineEdit {
    background-color: #ffffff;
    border: 2px solid #888;
    border-radius: 5px;
    padding: 8px;
    font-size: 14px;
    font-weight: bold;
    color: #333333;
}

QLineEdit:focus {
    border: 2px solid #0078d4;
    outline: none;
}

/* Style pour les cases à cocher */
QCheckBox {
    font-size: 14px;
    font-weight: bold;
    color: #333333;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 2px solid #888;
    border-radius: 3px;
    background-color: #ffffff;
}

QCheckBox::indicator:checked {
    background-color: #0078d4;
    border: 2px solid #0078d4;
}

/* Style pour les menus */
QMenuBar {
    background-color: #ffffff;
    color: #333333;
    font-size: 14px;
    font-weight: bold;
}

QMenuBar::item {
    background-color: transparent;
    padding: 8px 16px;
}

QMenuBar::item:selected {
    background-color: #0078d4;
    color: white;
}

QMenu {
    background-color: #ffffff;
    border: 2px solid #888;
    padding: 8px;
}

QMenu::item {
    padding: 8px 24px;
    font-size: 14px;
    font-weight: bold;
    color: #333333;
}

QMenu::item:selected {
    background-color: #0078d4;
    color: white;
}
QTableView::item:selected {
    background-color: #2196F3;
   color: rgb(0, 0, 0);
}</string>
