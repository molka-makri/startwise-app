      <string notr="true">/* 🎯 QGroupBox - Conteneur avec un style élégant et clair */
QGroupBox {
    background-color:rgb(255, 255, 255); /* Fond clair */
    border: 2px solid #0078D7; /* Bleu moderne */
    border-radius: 10px;
    margin-top: 20px; /* Décalage pour le titre */
    padding: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 5px 10px;
    background-color: #0078D7;
    color: white;
    font-weight: bold;
    border-radius: 5px;
}

/* 🏷️ QLabel (Texte en noir) */
QLabel {
    color: black;
    font-size: 14px;
}

/* 📝 QTextEdit */
QTextEdit {
    background-color: white;
    border: 2px solid #0078D7;
    border-radius: 8px;
    padding: 6px;
    color: black;
}

QTextEdit:focus {
    border-color: #005A9E;
}

/* 🎛️ QComboBox */
QComboBox {
    background-color: white;
    border: 2px solid #0078D7;
    border-radius: 6px;
    padding: 5px;
    color: black;
}

QComboBox:hover {
    border-color: #005A9E;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    background: #0078D7; /* Bleu clair, ou une autre couleur */
    border-radius: 4px;
    border: none;
}

/* Option pour améliorer l'apparence de la flèche */
QComboBox::down-arrow {
     /* Si tu veux une flèche personnalisée */
	image: url(:/img/images/bottom-right_10024180.png);
    width: 18px;
    height: 18px;
}

QComboBox QAbstractItemView {
    background-color: white;
    color: black;
    selection-background-color: #0078D7;
    border-radius: 6px;
}

/* 🔘 QPushButton */
QPushButton {
    background-color: #0078D7;
    border-radius: 8px;
    padding: 8px 15px;
    color: white;
    font-weight: bold;
    border: none;
}

QPushButton:hover {
    background-color: #005A9E;
}

QPushButton:pressed {
    background-color: #004080;
}

/* 🎚️ QSlider */
/* 🎚️ QSlider - Barre de progression claire et élégante */
QSlider::groove:horizontal {
    background: #ddd; /* Gris clair pour la barre */
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #0078D7; /* Bleu moderne */
    width: 16px;
    height: 16px;
    border-radius: 8px;
    margin: -5px 0;
}

QSlider::handle:horizontal:hover {
    background: #005A9E; /* Bleu plus foncé au survol */
}

QSlider::sub-page:horizontal {
    /*background: #0078D7;  Couleur de remplissage */
    border-radius: 3px;
}

/* 🎨 QLabel - Texte en noir avec fond transparent */
QLabel {
    background-color: transparent;  /* Supprime le fond gris */
    color: black; /* Texte noir pour une bonne lisibilité */
    font-size: 14px;
    font-weight: bold;
}

