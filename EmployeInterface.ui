        <string notr="true">/* Style pour le QGroupBox */
QGroupBox {
    background-color: #f9f9f9;
    border: 2px solid #0078d4;
    border-radius: 8px;
    padding: 15px;
    margin: 10px;
}

/* Titre du QGroupBox */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 5px;
    font-size: 18px;
    font-weight: bold;
    color: #0078d4;
}

/* Labels à l'intérieur du QGroupBox */
QGroupBox QLabel {
    color: black;
    font-size: 14px;
    font-weight: bold;
    margin-bottom: 5px;
}

/* Textes d'entrées à l'intérieur du QGroupBox */
QGroupBox QLineEdit,
QGroupBox QDateEdit, 
QGroupBox QTimeEdit {
    background-color: #f5f5f5;
	color: black;
    border: 1px solid #ccc;
    padding: 5px;
    border-radius: 3px;
    font-size: 14px;
}

/* Focus sur QLineEdit, QDateEdit, QTimeEdit */
QGroupBox QLineEdit:focus, 
QGroupBox QDateEdit:focus, 
QGroupBox QTimeEdit:focus {
    border-color: #0078d4;
    background-color: #e6f0ff;
}

/* Placeholders dans les champs de texte */
QGroupBox QLineEdit::placeholder, 
QGroupBox QDateEdit::placeholder, 
QGroupBox QTimeEdit::placeholder {
    color: black; /* Définit la couleur des placeholders à noir */
    font-style: italic;
}

/* Boutons radio dans le QGroupBox */
QGroupBox QRadioButton {
    color: black;
    font-size: 14px;
}

QGroupBox QRadioButton::indicator {
    width: 15px;
    height: 15px;
    border: 2px solid #0078d4;
    background-color: #e6e6e6;  /* Arrière-plan gris clair */
    border-radius: 50%;
}

QGroupBox QRadioButton::indicator:checked {
    background-color: #0078d4;
    border: 2px solid #0078d4;
}

QGroupBox QRadioButton::indicator:unchecked {
    background-color: #e6e6e6;
    border: 2px solid #ccc;
}

/* Boutons dans le QGroupBox */
QGroupBox QPushButton {
    background-color: #0078d4;
    color: white;
    padding: 10px;
    border-radius: 5px;
    border: none;
    font-size: 14px;
    cursor: pointer;
}

QGroupBox QPushButton:hover {
    background-color: #005a9e;
}

QGroupBox QPushButton:pressed {
    background-color: #003d6a;
}

/* Progress Bar dans le QGroupBox */
QGroupBox QProgressBar {
    border: 1px solid #ccc;
    border-radius: 5px;
    text-align: center;
    background-color: #f5f5f5;
    color: black;
}

QGroupBox QProgressBar::chunk {
    background-color: #0078d4;
    border-radius: 5px;
}
