QComboBox::down-arrow {
    image: url(:/icons/black_arrow.svg);  /* Assurez-vous que l'icône est noire */
    width: 16px;
    height: 16px;
}

QComboBox:hover {
    border: 1px solid rgb(0, 122, 255);
    background-color: rgb(240, 240, 240);
    box-shadow: 0 2px 6px rgba(0, 122, 255, 0.3);
}

QComboBox:focus {
    border: 2px solid rgb(0, 122, 255);
    background-color: rgb(255, 255, 255);
    box-shadow: 0 0 8px rgba(0, 122, 255, 0.5);
}

QComboBox QAbstractItemView {
    background-color: rgb(245, 245, 245);
    border: 1px solid rgb(200, 200, 200);
    color: #333;
    selection-background-color: rgb(0, 122, 255);
    selection-color: white;
    border-radius: 8px;
    padding: 5px;
    margin-top: 2px;
    max-height: 300px;
}

QComboBox::drop-down {
    border: none;
    background-color: transparent;
}

QComboBox QAbstractItemView::item {
    padding: 8px;
    color: #555;
    font-size: 14px;
    transition: background-color 0.2s ease;
}

QComboBox QAbstractItemView::item:selected {
    background-color: rgb(0, 122, 255);
    color: white;
    font-weight: bold;
}

QComboBox QAbstractItemView::item:hover {
    background-color: rgb(0, 122, 255);  /* Survol des éléments */
    color: white;
}

/* Ajout de la couleur pour l'élément sélectionné */
QComboBox::item:selected {
    background-color: rgb(0, 122, 255);  /* Assurez-vous que la couleur de fond de l'élément sélectionné est bien bleue */
    color: white;  /* Le texte doit être blanc */
}
